{
  "version": 2,
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/app.py"
    }
  ],
  "redirects": [
    {
      "source": "/privacy/",
      "destination": "/announcement",
      "permanent": false
    }
  ],
  "trailingSlash": true
}
